#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/tmp/price-matcher-final/node_modules/.pnpm/mkdirp@3.0.1/node_modules/mkdirp/dist/cjs/src/node_modules:/tmp/price-matcher-final/node_modules/.pnpm/mkdirp@3.0.1/node_modules/mkdirp/dist/cjs/node_modules:/tmp/price-matcher-final/node_modules/.pnpm/mkdirp@3.0.1/node_modules/mkdirp/dist/node_modules:/tmp/price-matcher-final/node_modules/.pnpm/mkdirp@3.0.1/node_modules/mkdirp/node_modules:/tmp/price-matcher-final/node_modules/.pnpm/mkdirp@3.0.1/node_modules:/tmp/price-matcher-final/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/tmp/price-matcher-final/node_modules/.pnpm/mkdirp@3.0.1/node_modules/mkdirp/dist/cjs/src/node_modules:/tmp/price-matcher-final/node_modules/.pnpm/mkdirp@3.0.1/node_modules/mkdirp/dist/cjs/node_modules:/tmp/price-matcher-final/node_modules/.pnpm/mkdirp@3.0.1/node_modules/mkdirp/dist/node_modules:/tmp/price-matcher-final/node_modules/.pnpm/mkdirp@3.0.1/node_modules/mkdirp/node_modules:/tmp/price-matcher-final/node_modules/.pnpm/mkdirp@3.0.1/node_modules:/tmp/price-matcher-final/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mkdirp/dist/cjs/src/bin.js" "$@"
else
  exec node  "$basedir/../mkdirp/dist/cjs/src/bin.js" "$@"
fi
